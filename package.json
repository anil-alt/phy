{
  "name": "phy",
  "version": "4.0.0",
  "description": "Phy is a set of minimalist hyperscript helpers for Preact",
  "license": "BSD-3-Clause",
  "main": "lib/phy.js",
  "homepage": "https://github.com/groupon/phy",
  "repository": {
    "type": "git",
    "url": "https://github.com/groupon/phy"
  },
  "bugs": {
    "url": "https://github.com/groupon/phy/issues"
  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "pretest": "npm-run-all pretest:*",
    "test": "npm-run-all test:*",
    "posttest": "npm-run-all posttest:*",
    "pretest:lint": "npm-run-all lint:*",
    "posttest:nlm": "nlm verify",
    "test:unit": "nyc mocha"
  },
  "engines": {
    "node": ">=8",
    "npm": "^6.0.0"
  },
  "nlm": {
    "license": {
      "files": [
        "lib"
      ]
    }
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "branches": 100,
    "functions": 100,
    "statements": 100
  },
  "dependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "assertive": "^2.1.0",
    "eslint": "^6.2.1",
    "eslint-config-groupon": "^7.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.2.0",
    "nlm": "^3.6.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "preact-render-to-string": "^5.0.6",
    "prettier": "^1.18.2"
  },
  "author": {
    "name": "David Bushong",
    "email": "dbushong@groupon.com"
  },
  "files": [
    "*.js",
    "lib",
    "preact"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
